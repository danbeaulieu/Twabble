body,
	html {
		margin:0;
		padding:0;
		color:#000;
		background-color: #F8F8FF;
		
	}

#container {
  margin:0 auto;
  width:100%;
  float: left;
  line-height: 130%;
}

#content {
  width: 69%;
  padding:1%;
  background-color: #F8F8FF;
  float: left;
}

#track {
	
	background-color: #CCCCFF;
	padding: 2px 15px;
}

#content a {
	color: #000;
	text-decoration: none;
}

#track_list ul {
list-style-type: none;
padding: 5px;
border-right: 1px solid #000;

}
#track_list li {
padding: 5px 15px;
border-top: 1px dashed #BBB;
height:61px;


}

#voting {
float:left;
height:50px;
font-size:25px;
margin:5px;
text-align:center;
width:30px;
}


#nav ul
{
padding: .2em 0;
margin: 0;
list-style-type: none;
background-color: #BBBBBB;
color: #FFF;
width: 100%;
font: normal 90% arial, helvetica, sans-serif;
text-align: left;
}

#nav li { display: inline;
	}

#nav li a
{
text-decoration: none;
background-color: #BBBBBB;
color: #000;
padding: .2em 1em;
border-right: 1px solid #fff;
}

#nav li a:hover
{
background-color: #fff;
color: #000;
}

#right {
  float:right;
  width:27%;
  padding:1%;
  background-color: #F8F8FF;

}

#header {
	background-color: #000000;
	color: #FFFFFF;
	padding:5px 10px;

}

#footer {
	background-color: #F8F8FF;;
	border-top-style: dashed;
	border-width: 1px;
    clear: both;
	text-align: center;
}
/*** IE6 Fix ***/
* html #left {
  left: 150px;           /* RC width */
}